<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:f="sap.f" xmlns="sap.uxap" xmlns:layout="sap.ui.layout"
	xmlns:form="sap.ui.layout.form" controllerName="gvdpl.training.sandboxApp.controller.Detail" xmlns:html="http://www.w3.org/1999/xhtml">
	<ObjectPageLayout>
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title text="Denise Smith" wrapping="true" class="sapUiSmallMarginEnd"/>
				</expandedHeading>
				<snappedHeading>
					
				</snappedHeading>
				<navigationActions>
					<m:OverflowToolbarButton type="Transparent" icon="sap-icon://full-screen" press="handleFullScreen" tooltip="Enter Full Screen Mode" visible="true"/>
					<m:OverflowToolbarButton type="Transparent" icon="sap-icon://exit-full-screen" press="handleExitFullScreen" tooltip="Exit Full Screen Mode" visible="true"/>
					<m:OverflowToolbarButton type="Transparent" icon="sap-icon://decline" press="handleClose"  tooltip="Close column" visible="true"/>
				</navigationActions>
			</ObjectPageDynamicHeaderTitle>
			<!--<ObjectPageHeader objectImageURI="{value: 'gvdpl/training/sandboxApp/image/avatar.jpg', formatter: 'sap.ui.require.toUrl'}"-->
			<!--	objectTitle="Denise Smith" objectImageShape="Circle" objectImageAlt="Denise Smith" objectSubtitle="Senior UI Developer"-->
