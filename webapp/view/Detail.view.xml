			<!--</ObjectPageHeader>-->
		</headerTitle>
		<headerContent>
			<layout:HorizontalLayout>
				<m:Label text="{i18n>empId}"/>
				<m:Text text="abc"/>
				<m:Label text="{i18n>empDateOfBirth}"/>
				<m:Text text="abc"/>
				<m:Label text="{i18n>empPlaceOfBirth}"/>
				<m:Text text="abc"/>
			</layout:HorizontalLayout>
		</headerContent>
		<sections>
			<ObjectPageSection title="IT0006">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<form:SimpleForm editable="false" layout="ColumnLayout">
								<m:Label text="{i18n>empAddress}" design="Bold"/>
								<m:Text text="{DetailsModel>/Address}"/>
								<m:Label text="{i18n>empPostCode}" design="Bold"/>
								<m:Text text="{DetailsModel>/KodPocztowy}"/>
								<m:Label text="{i18n>empPostSpot}" design="Bold"/>
								<m:Text text="{DetailsModel>/MiejscPoczt}"/>
							</form:SimpleForm>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="IT0016">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<form:SimpleForm editable="false" layout="ColumnLayout">
								<m:Label text="{i18n>empContractType}" design="Bold"/>
								<m:Text text="{DetailsModel>/RodzajUmowy}"/>
								<m:Label text="{i18n>empContractDateFrom}" design="Bold"/>
								<m:Text text="{DetailsModel>/DataZawUmow}"/>
								<m:Label text="{i18n>empContractDateTo}" design="Bold"/>
								<m:Text text="{DetailsModel>/DataKonUmow}"/>
							</form:SimpleForm>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="IT0022">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<form:SimpleForm editable="false" layout="ColumnLayout">
								<m:Label text="{i18n>empEducation}" design="Bold"/>
								<m:Text text="{DetailsModel>/Wyksztalcenie}"/>
								<m:Label text="{i18n>empUniversityName}" design="Bold"/>
								<m:Text text="{DetailsModel>/NazwaSzkoly}"/>
								<m:Label text="{i18n>empEducationFinishDate}" design="Bold"/>
								<m:Text text="{DetailsModel>/DataUkonSzkol}"/>
								<m:Label text="{i18n>empMajor}" design="Bold"/>
								<m:Text text="{DetailsModel>/KierunNaukTxt}"/>
							</form:SimpleForm>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
